<T3DataStructure>
	<meta>
		<langDisable>1</langDisable>
	</meta>
	<sheets>
		<general>
			<ROOT>
				<TCEforms>
					<sheetTitle>LLL:EXT:imagecycle/locallang_db.xml:tt_content.pi_flexform.sheet_general</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>
					<mode>
						<TCEforms>
							<onChange>reload</onChange>
							<label>LLL:EXT:imagecycle/locallang_db.xml:tt_content.pi_flexform.mode</label>
							<config>
								<type>select</type>
								<size>1</size>
								<maxitems>1</maxitems>
								<itemsProcFunc>tx_imagecycle_itemsProcFunc->getModes</itemsProcFunc>
							</config>
						</TCEforms>
					</mode>

					<images>
						<TCEforms>
							<displayCond>FIELD:mode:!IN:dam,dam_catedit,uploadRTE</displayCond>
							<label>LLL:EXT:imagecycle/locallang_db.xml:tt_content.pi_flexform.images</label>
							<config>
								<type>group</type>
								<internal_type>file</internal_type>
								<allowed>files</allowed>
								<size>12</size>
								<maxitems>1000</maxitems>
								<minitems>0</minitems>
								<show_thumbs>1</show_thumbs>
								<allowed>gif,jpg,jpeg,png</allowed>
								<max_size>10240</max_size>
								<uploadfolder>uploads/tx_imagecycle</uploadfolder>
							</config>
						</TCEforms>
					</images>
					<hrefs>
						<TCEforms>
							<displayCond>FIELD:mode:!IN:dam,dam_catedit,uploadRTE</displayCond>
							<label>LLL:EXT:imagecycle/locallang_db.xml:tt_content.pi_flexform.hrefs</label>
							<config>
								<type>text</type>
								<wrap>OFF</wrap>
								<cols>48</cols>
								<rows>6</rows>
							</config>
						</TCEforms>
					</hrefs>
					<captions>
						<TCEforms>
							<displayCond>FIELD:mode:!IN:dam,dam_catedit,uploadRTE</displayCond>
							<label>LLL:EXT:imagecycle/locallang_db.xml:tt_content.pi_flexform.captions</label>
							<config>
								<type>text</type>
								<wrap>OFF</wrap>
								<cols>48</cols>
								<rows>6</rows>
							</config>
						</TCEforms>
					</captions>

					<damimages>
						<TCEforms>
							<displayCond>FIELD:mode:=:dam</displayCond>
							<label>LLL:EXT:imagecycle/locallang_db.xml:tt_content.pi_flexform.damimages</label>
							<config>
								<type>group</type>
								<form_type>user</form_type>
								<userFunc><![CDATA[EXT:dam/lib/class.tx_dam_tcefunc.php:&tx_dam_tceFunc->getSingleField_typeMedia]]></userFunc>
								<userProcessClass><![CDATA[EXT:mmforeign/class.tx_mmforeign_tce.php:tx_mmforeign_tce]]></userProcessClass>
								<internal_type>db</internal_type>
								<allowed>tx_dam</allowed>
								<allowed_types>gif,jpg,jpeg,png</allowed_types>
								<prepend_tname>1</prepend_tname>
								<MM>tx_dam_mm_ref</MM>
								<MM_foreign_select>1</MM_foreign_select>
								<MM_opposite_field>1</MM_opposite_field>
								<MM_match_fields>
									<ident>imagecycle</ident>
								</MM_match_fields>
								<show_thumbs>1</show_thumbs>
								<size>10</size>
								<maxitems>1000</maxitems>
								<minitems>0</minitems>
								<autoSizeMax>30</autoSizeMax>
							</config>
						</TCEforms>
					</damimages>

					<damcategories>
						<TCEforms>
							<displayCond>FIELD:mode:=:dam_catedit</displayCond>
							<label>LLL:EXT:imagecycle/locallang_db.xml:tt_content.pi_flexform.damcategories</label>
							<config>
								<type>select</type>
								<form_type>user</form_type>
								<userFunc>EXT:dam/lib/class.tx_dam_tcefunc.php:tx_dam_tceFunc->getSingleField_selectTree</userFunc>
								<treeViewClass>EXT:dam/components/class.tx_dam_selectionCategory.php:tx_dam_selectionCategory</treeViewClass>
								<foreign_table>tx_dam_cat</foreign_table>
								<size>5</size>
								<autoSizeMax>25</autoSizeMax>
								<minitems>0</minitems>
								<maxitems>99</maxitems>
								<default></default>
							</config>
						</TCEforms>
					</damcategories>

					<imagesRTE>
						<tx_templavoila>
							<title>LLL:EXT:imagecycle/locallang_db.xml:tt_content.pi_flexform.uploadRTE</title>
						</tx_templavoila>
						<type>array</type>
						<section>1</section>
						<el>
							<data>
								<type>array</type>
								<el>
									<image>
										<TCEforms>
											<label>LLL:EXT:imagecycle/locallang_db.xml:tt_content.pi_flexform.image</label>
											<config>
												<type>group</type>
												<internal_type>file</internal_type>
												<allowed>files</allowed>
												<size>1</size>
												<maxitems>1</maxitems>
												<minitems>0</minitems>
												<show_thumbs>1</show_thumbs>
												<allowed>gif,jpg,jpeg,png</allowed>
												<max_size>10240</max_size>
												<uploadfolder>uploads/tx_imagecycle</uploadfolder>
											</config>
										</TCEforms>
									</image>
									<href>
										<TCEforms>
											<label>LLL:EXT:imagecycle/locallang_db.xml:tt_content.pi_flexform.href</label>
											<config>
												<type>input</type>
												<size>15</size>
												<max>255</max>
												<eval>trim</eval>
												<wizards>
													<_PADDING>2</_PADDING>
													<link>
														<type>popup</type>
														<title>Link</title>
														<icon>link_popup.gif</icon>
														<script>browse_links.php?mode=wizard</script>
														<JSopenParams>height=500,width=500,status=0,menubar=0,scrollbars=1</JSopenParams>
													</link>
												</wizards>
											</config>
										</TCEforms>
									</href>
									<caption>
										<TCEforms>
											<label>LLL:EXT:imagecycle/locallang_db.xml:tt_content.pi_flexform.caption</label>
											<config>
												<type>text</type>
												<cols>30</cols>
												<rows>5</rows>
											</config>
											<defaultExtras>richtext[]:rte_transform[mode=ts_css]</defaultExtras>
										</TCEforms>
									</caption>
								</el>
							</data>
						</el>
					</imagesRTE>
					<hideRTE>
						<TCEforms>
							<displayCond>FIELD:mode:!=:uploadRTE</displayCond>
							<label></label>
							<config>
								<type>user</type>
								<userFunc>tx_imagecycle_TCAform->hideRTE</userFunc>
							</config>
						</TCEforms>
					</hideRTE>
				</el>
			</ROOT>
		</general>
		<settings>
			<ROOT>
				<TCEforms>
					<sheetTitle>LLL:EXT:imagecycle/locallang_db.xml:tt_content.pi_flexform.sheet_settings</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>
					<coinEffect>
						<TCEforms>
							<exclude>1</exclude>
							<label>LLL:EXT:imagecycle/locallang_db.xml:tt_content.pi_flexform.coinEffect</label>
							<config>
								<type>select</type>
								<size>1</size>
								<maxitems>1</maxitems>
								<items type="array">
									<numIndex index="0" type="array">
										<numIndex index="0">LLL:EXT:imagecycle/locallang_db.xml:tt_content.pi_flexform.from_ts</numIndex>
										<numIndex index="1"></numIndex>
									</numIndex>
								</items>
								<itemsProcFunc>tx_imagecycle_itemsProcFunc->getEffectsCoin</itemsProcFunc>
							</config>
						</TCEforms>
					</coinEffect>
					<imagewidth>
						<TCEforms>
							<exclude>1</exclude>
							<label>LLL:EXT:imagecycle/locallang_db.xml:tt_content.pi_flexform.imagewidth</label>
							<config>
								<type>input</type>
								<size>16</size>
								<max>16</max>
								<eval>nospace</eval>
							</config>
						</TCEforms>
					</imagewidth>
					<imageheight>
						<TCEforms>
							<exclude>1</exclude>
							<label>LLL:EXT:imagecycle/locallang_db.xml:tt_content.pi_flexform.imageheight</label>
							<config>
								<type>input</type>
								<size>16</size>
								<max>16</max>
								<eval>nospace</eval>
							</config>
						</TCEforms>
					</imageheight>
					<coinSpw>
						<TCEforms>
							<exclude>1</exclude>
							<label>LLL:EXT:imagecycle/locallang_db.xml:tt_content.pi_flexform.coinSpw</label>
							<config>
								<type>input</type>
								<size>2</size>
								<max>2</max>
								<eval>integer</eval>
							</config>
						</TCEforms>
					</coinSpw>
					<coinSph>
						<TCEforms>
							<exclude>1</exclude>
							<label>LLL:EXT:imagecycle/locallang_db.xml:tt_content.pi_flexform.coinSph</label>
							<config>
								<type>input</type>
								<size>2</size>
								<max>2</max>
								<eval>integer</eval>
							</config>
						</TCEforms>
					</coinSph>
					<coinDelay>
						<TCEforms>
							<exclude>1</exclude>
							<label>LLL:EXT:imagecycle/locallang_db.xml:tt_content.pi_flexform.coinDelay</label>
							<config>
								<type>input</type>
								<size>6</size>
								<max>6</max>
								<eval>integer</eval>
							</config>
						</TCEforms>
					</coinDelay>
					<coinSDelay>
						<TCEforms>
							<exclude>1</exclude>
							<label>LLL:EXT:imagecycle/locallang_db.xml:tt_content.pi_flexform.coinSDelay</label>
							<config>
								<type>input</type>
								<size>4</size>
								<max>4</max>
								<eval>integer</eval>
							</config>
						</TCEforms>
					</coinSDelay>
					<coinTitleSpeed>
						<TCEforms>
							<exclude>1</exclude>
							<label>LLL:EXT:imagecycle/locallang_db.xml:tt_content.pi_flexform.coinTitleSpeed</label>
							<config>
								<type>input</type>
								<size>4</size>
								<max>4</max>
								<eval>integer</eval>
							</config>
						</TCEforms>
					</coinTitleSpeed>
					<coinOpacity>
						<TCEforms>
							<exclude>1</exclude>
							<label>LLL:EXT:imagecycle/locallang_db.xml:tt_content.pi_flexform.coinOpacity</label>
							<config>
								<form_type>user</form_type>
								<userFunc><![CDATA[EXT:jftcaforms/lib/class.tx_jftcaforms_tceFunc.php:&tx_jftcaforms_tceFunc->getExtSlider]]></userFunc>
								<width>300</width>
								<range>
									<lower>0</lower>
									<upper>1</upper>
								</range>
								<decimalPrecision>2</decimalPrecision>
								<emptyValue>1</emptyValue>
							</config>
						</TCEforms>
					</coinOpacity>
					<coinNavigation>
						<TCEforms>
							<exclude>1</exclude>
							<label>LLL:EXT:imagecycle/locallang_db.xml:tt_content.pi_flexform.coinNavigation</label>
							<config>
								<form_type>user</form_type>
								<userFunc><![CDATA[EXT:imagecycle/lib/class.tx_imagecycle_tceFunc.php:&tx_imagecycle_tceFunc->getCheckbox]]></userFunc>
								<checked>0</checked>
							</config>
						</TCEforms>
					</coinNavigation>
					<coinLinks>
						<TCEforms>
							<exclude>1</exclude>
							<label>LLL:EXT:imagecycle/locallang_db.xml:tt_content.pi_flexform.coinLinks</label>
							<config>
								<form_type>user</form_type>
								<userFunc><![CDATA[EXT:imagecycle/lib/class.tx_imagecycle_tceFunc.php:&tx_imagecycle_tceFunc->getCheckbox]]></userFunc>
								<checked>0</checked>
							</config>
						</TCEforms>
					</coinLinks>
					<coinHoverPause>
						<TCEforms>
							<exclude>1</exclude>
							<label>LLL:EXT:imagecycle/locallang_db.xml:tt_content.pi_flexform.coinHoverPause</label>
							<config>
								<form_type>user</form_type>
								<userFunc><![CDATA[EXT:imagecycle/lib/class.tx_imagecycle_tceFunc.php:&tx_imagecycle_tceFunc->getCheckbox]]></userFunc>
								<checked>1</checked>
							</config>
						</TCEforms>
					</coinHoverPause>
				</el>
			</ROOT>
		</settings>
		<special>
			<ROOT>
				<TCEforms>
					<sheetTitle>LLL:EXT:imagecycle/locallang_db.xml:tt_content.pi_flexform.sheet_special</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>
					<options>
						<TCEforms>
							<exclude>1</exclude>
							<label>LLL:EXT:imagecycle/locallang_db.xml:tt_content.pi_flexform.optionsCoin</label>
							<config>
								<type>text</type>
								<wrap>OFF</wrap>
								<cols>48</cols>
								<rows>10</rows>
							</config>
						</TCEforms>
					</options>
					<optionsOverride>
						<TCEforms>
							<exclude>1</exclude>
							<label>LLL:EXT:imagecycle/locallang_db.xml:tt_content.pi_flexform.optionsOverride</label>
							<config>
								<type>check</type>
								<default>0</default>
							</config>
						</TCEforms>
					</optionsOverride>
				</el>
			</ROOT>
		</special>
	</sheets>
</T3DataStructure>

